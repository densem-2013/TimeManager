    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Department">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        rt.Name, COUNT(rt.Name) AS EXPR1
FROM            Request AS r INNER JOIN
                         RequestType AS rt ON rt.ID = r.TypeId INNER JOIN
                         Phase AS ph ON ph.RequestId = r.ID INNER JOIN
                         Project AS p ON p.PhaseId = ph.ID INNER JOIN
                         Status AS s ON s.ID = p.CurrentStatusId INNER JOIN
                             (SELECT        ProjectBudgetId, DepartmentId, SUM(AmountOfHours) AS AmountOfHours, SUM(AmountOfMoney) AS AmountOfMoney
                               FROM            ProjectBudgetAllocation
                               GROUP BY ProjectBudgetId, DepartmentId) AS pba ON pba.ProjectBudgetId = ph.BudgetId INNER JOIN
                         Department AS d ON d.ID = pba.DepartmentId LEFT OUTER JOIN
                         vwSpentResources AS sr ON sr.ProjectID = p.ID AND sr.DepartmentID = d.ID
WHERE        (s.ID IN (131073, 131070)) AND (sr.DepartmentID = @Department)
GROUP BY rt.Name</CommandText>
      </Query>
      <Fields>
