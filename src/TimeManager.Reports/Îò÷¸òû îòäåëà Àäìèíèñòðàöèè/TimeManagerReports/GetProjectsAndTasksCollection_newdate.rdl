  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Department">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
 
 '' AS 'RowNumber', 
 p.Name AS 'ProjectName',
 cu.Name AS 'Customer',
 rt.Name AS 'RequestTypeName',
 c.Number AS 'ContractName',
 p.Code AS 'ProjectCode',
 r.Number AS 'RequestNumber',
 pp.Name AS 'RequestPriority',
 s.Name AS 'ProjectStatusName',
 dbo.fnGetEmployeeShortNameByID(r.ProjectManagerId) AS 'ProjectManager',
 d.ShortName AS 'Department',
 ph.DeadLine AS 'PlanedFinishDate',
 r.StartDate AS 'StartDate',
 '19.05.2012' AS 'RealFinishDate',
 pba.AmountOfHours AS 'PlanedHours',
 sr.SpentTime AS 'SpentHours',
                sr.SpentMoney AS 'Expences',
               pba.AmountOfMoney AS 'Budget',
CAST ((CASE pba.AmountOfHours when 0 then -1 else(sr.SpentTime/ pba.AmountOfHours*100) END ) as integer) AS 'PercentOfUsedBudget'

 
 From Request r
 inner join RequestType rt on rt.ID = r.TypeId
 inner join Contract c on c.ID = r.ContractId
 inner join Customer cu on cu.ID=c.CustomerId
 inner join Phase ph on ph.RequestId=r.ID
 inner join Project p on p.PhaseId=ph.ID
 inner join ProjectPriority pp on pp.ID=p.ProjectPriorityId
 inner join Status s on s.ID=p.CurrentStatusId
 inner join ContractType ct on ct.ID=c.ContractTypeId
 inner join 
 ( select ProjectBudgetId, DepartmentId, Sum(AmountOfHours) as AmountOfHours, Sum(AmountOfMoney) AS AmountOfMoney  from ProjectBudgetAllocation 
 group by  ProjectBudgetId, DepartmentId)
  pba on pba.ProjectBudgetId=ph.BudgetId
 inner join Department d on d.ID=pba.DepartmentId
 left join vwSpentResources sr on sr.ProjectID=p.ID and sr.DepartmentID=d.ID
where s.ID  in (131072,131073,131076,131070,131071) and sr.DepartmentID=@Department</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
